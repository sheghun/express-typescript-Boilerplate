{
	"name": "myragpblog",
	"version": "1.0.0",
	"description": "Myragpblog Backend Application",
	"main": "src/server.js",
	"scripts": {
		"test": "jest --forceExit --coverage --verbose",
		"build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
		"watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
		"watch-node": "NODE_ENV=development nodemon dist/entry.js",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"watch-ts": "tsc -w",
		"build-ts": "tsc",
		"serve-debug": "NODE_ENV=development nodemon --inspect dist/entry.js",
		"copy-static-assets": "ts-node copyStaticAssets.js"
	},
	"author": "Oladiran Segun Solomon <sheghunoladiran9@gmail.com>",
	"license": "ISC",
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/bluebird": "^3.5.27",
		"@types/cookie-parser": "^1.4.1",
		"@types/cors": "^2.8.5",
		"@types/dotenv": "^6.1.1",
		"@types/errorhandler": "^0.0.32",
		"@types/express": "^4.17.0",
		"@types/express-validator": "^3.0.0",
		"@types/jest": "^24.0.15",
		"@types/jsonwebtoken": "^8.3.2",
		"@types/mysql2": "types/mysql2",
		"@types/node": "^12.0.12",
		"@types/nodemailer": "^6.2.0",
		"@types/shelljs": "^0.8.5",
		"@types/validator": "^10.11.1",
		"concurrently": "^4.1.1",
		"jasmine-await": "^0.0.3",
		"jasmine-ts": "^0.3.0",
		"jest": "^24.8.0",
		"jest-cli": "^24.8.0",
		"nodemon": "^1.19.1",
		"shelljs": "^0.8.3",
		"ts-jest": "^24.0.2",
		"ts-node": "^8.3.0",
		"tslint": "^5.18.0"
	},
	"dependencies": {
		"bcrypt": "^3.0.6",
		"body-parser": "^1.19.0",
		"chalk": "^2.4.2",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"dotenv": "^8.0.0",
		"errorhandler": "^1.5.1",
		"esm": "^3.2.25",
		"express": "^4.17.1",
		"express-validator": "^5.3.1",
		"jsonwebtoken": "^8.5.1",
		"mysql": "^2.17.1",
		"mysql2": "^1.6.5",
		"nodemailer": "^6.2.1",
		"sequelize": "^5.8.9",
		"sequelize-cli": "^5.5.0",
		"typescript": "^3.5.2"
	}
}