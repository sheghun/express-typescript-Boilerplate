{
  "name": "myragpblog",
  "version": "1.0.0",
  "description": "Express Typescript Boilerplate Application",
  "main": "src/server.js",
  "scripts": {
    "test": "jest --forceExit --coverage --verbose",
    "build": "yarn run build-ts && yarn run eslint && yarn run create-npm-lock-file && yarn run copy-static-assets",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "NODE_ENV=development nodemon src/server.js",
    "eslint": "eslint -c .eslintrc.js \"**/*.ts\" --ignore-pattern node_modules",
    "create-npm-lock-file": "synp -f -s yarn.lock",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "serve-debug": "NODE_ENV=development nodemon --inspect src/server.js",
    "copy-static-assets": "ts-node copyStaticAssets.ts"
  },
  "author": "Oladiran Segun Solomon <sheghunoladiran9@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.1",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^12.7.3",
    "@types/nodemailer": "^6.2.1",
    "@types/shelljs": "^0.8.5",
    "@types/validator": "^10.11.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "concurrently": "^4.1.2",
    "eslint": "^6.3.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jasmine-await": "^0.0.3",
    "jasmine-ts": "^0.3.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^9.0.0",
    "mem": "^6.0.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "nodemailer": "^6.3.0",
    "sequelize": "^5.17.2",
    "sequelize-cli": "^5.5.0",
    "typescript": "^3.6.2",
    "winston": "^3.2.1"
  }
}
